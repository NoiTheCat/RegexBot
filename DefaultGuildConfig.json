  "RegexModerator": [
    {
      "Label": "No cheese",
      "Regex": "cheese",
      "Response": [
        "say #_ You can't say that, that's illegal",
        "delete"
      ]
    },
    {
      "Label": "Secret club initiation",
      "Regex": "my name is .* and I dislike cheese",
      "Response": [
        "say @_ We welcome you.",
        "addrole &00000::Secret Club member"
      ]
    }
  ],

  "AutoResponder": [
    {
      "Label": "Infinite no u",
      "Regex": "no u",
      "Reply": "no u"
    },
    {
      "Label": "Acknowledge praise",
      "Regex": "yes u",
      "Reply": ":blush:"
    }
  ]
}